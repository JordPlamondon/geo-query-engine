{
  "name": "geo-query-engine",
  "version": "0.1.0",
  "description": "A high-performance client-side geospatial search and filtering engine for location-based applications",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "benchmark": "tsx benchmarks/index.ts",
    "playground": "vite --config playground/vite.config.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "geospatial",
    "geo",
    "location",
    "search",
    "filter",
    "query",
    "spatial",
    "rtree",
    "haversine",
    "distance",
    "typescript"
  ],
  "author": "Jordan Plamondon",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JordPlamondon/geo-query-engine.git"
  },
  "bugs": {
    "url": "https://github.com/JordPlamondon/geo-query-engine/issues"
  },
  "homepage": "https://github.com/JordPlamondon/geo-query-engine#readme",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/rbush": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.0",
    "tsup": "^8.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.4.0",
    "vite": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "dependencies": {
    "rbush": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
